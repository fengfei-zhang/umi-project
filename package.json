{
  "name": "project-name-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "project-name-frontend",
  "scripts": {
    "cm": "git-cz",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "release": "standard-version",
    "deploy:dev": "deploy-cli-service deploy --mode dev"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx}": "npm run lint",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "npm run prettier",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@antelopecloud/utils": "^1.1.4",
    "@types/classnames": "^2.3.1",
    "@types/mockjs": "^1.0.4",
    "@types/validator": "^13.7.0",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "^1.8.6",
    "ahooks": "^2.10.6",
    "antd": "^4.17.3",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "classnames-loader": "^2.1.0",
    "html5-player": "^0.5.10",
    "js-base64": "^3.6.1",
    "lodash": "^4.17.21",
    "ly-player": "0.7.5",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-interface-builder": "^0.3.0",
    "ts-interface-checker": "^1.0.0",
    "ts-md5": "^1.2.9",
    "umi": "^3.5.10",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/lodash": "^4.14.168",
    "@umijs/fabric": "^2.0.6",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.25.0",
    "husky": "^4.2.5",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "slash2": "^2.0.0",
    "typescript": "^3.5.3"
  },

  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
